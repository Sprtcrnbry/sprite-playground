{
  "name": "learn",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "bun --hot vite",
    "build": "bun run type-check && bun run build-only",
